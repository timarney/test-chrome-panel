{
  "name": "App Context",
  "description": "Adds panel to the Chrome Developer Tools to show App context.",
  "version": "1.5",
  "manifest_version": 2,
  "devtools_page": "devtools.html",
  "externally_connectable": {
    "matches": ["http://localhost:*/*", "https://codesandbox.io:*/*"]
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "icons/16-production.png",
    "32": "icons/32-production.png",
    "48": "icons/48-production.png",
    "128": "icons/128-production.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16-production.png",
      "32": "icons/32-production.png",
      "48": "icons/48-production.png",
      "128": "icons/128-production.png"
    }
  }
}
